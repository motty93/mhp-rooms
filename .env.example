# 開発環境用の環境変数テンプレート
# このファイルを .env.local にコピーして使用してください

# アプリケーション設定
PORT=8080
ENV=development

# データベース設定
DATABASE_URL=postgres://mhp_user:mhp_password@db:5432/mhp_rooms_dev?sslmode=disable
DB_HOST=db
DB_PORT=5432
DB_NAME=mhp_rooms_dev
DB_USER=mhp_user
DB_PASSWORD=mhp_password

# Redis設定
REDIS_URL=redis://redis:6379
REDIS_HOST=redis
REDIS_PORT=6379

# Supabase設定（実装時に使用）
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# セッション設定
SESSION_SECRET=your_session_secret_key_change_in_production

# ログレベル
LOG_LEVEL=debug